<style>

</style>
<body style="padding-top:0px">       
 <div style="width:900px;margin:0 auto;">

   <div class="header">退订用户信息
      <small>(<%= (@unsub ? @unsub.count : 0 ) %>人次)</small>
       <small style="float: right;">
       <%= link_to "导出数据","/report/export.xls?c_id=#{params[:c_id]}&keyword=unsub",:class=>"focus-link"%>
       </small>
       </div>
   <table class="table table-striped">
     <thead class="focus-thead">
       <tr>
         <th width="50%">会员信息</th>
         <th width="50%">退订时间</th>
       </tr>
     </thead>
     <tbody>
      <% if @unsub %>
       <% @unsub.each do |unsub| %>
         <tr>
           <td width="40%"><%= unsub.email %></td>
           <td><%= unsub.created_at.to_s %></td>
         </tr>
       <% end %>
      <% else %>
         <tr>
           <td colspan="2" style="text-align: center">退订记录为空</td>
         </tr>
      <% end %>
     </tbody>
   </table>


 </div>

</body>
