 <body style="padding-top:0px">       
    <div  style="margin:0 auto;width:900px;float:none;text-align:center;margin-bottom:5px">
      <div>
         <span class="btn" onclick="$('#chart_hour_div').css('display','block');$('#chart_week_div').css('display','none');">Show By Hour</span>
         <span class="btn" onclick="$('#chart_hour_div').css('display','none');$('#chart_week_div').css('display','block');">Show By Week</span>
      </div>
    </div>
     
   <div id="chart_hour_div" style="margin:0 auto;width:900px;float:none;text-align:center;">
     <%= high_chart("chart_hour", g_chart_2y(get_hour_data(params[:c_id]))) %>
   </div>
   <div id="chart_week_div" style="margin:0 auto;width:900px;display:none;">
     <%= high_chart("chart_week", g_chart_2y(get_week_data(params[:c_id]))) %>
   </div>
 
 </body>

