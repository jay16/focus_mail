
<table id="report_table" class="table table-striped">
  <thead>
    <tr>
      <th>邮件组名</th>
      <th>邮件总数</th>
      <th>打开人数</th>
      <th>点击次数</th>
      <th>报表</th>
      <th>打开邮件名单</th>
      <th>点击>=2名单</th>
    </tr>
  </thead>
  <tbody>
    <% @campaign_click_report.each do |report| %>
      <tr>
        <td><%= report[0] %></td>
        <td><%= report[1] %></td>
        <td><%= report[2] %></td>
        <td><%= report[3] %></td>
        <td><%= link_to '报表', {:controller=>'report',:action=>'campaign',:id => report[4] }, :remote => true, :class => 'btn btn-primary' %></td>
        <td><%= link_to '名单', {:controller=>'report',:action=>'click_list',:campaign_id => report[4] }, :remote => true, :class => 'btn btn-primary' %></td>
      <td><%= link_to '潜在客户', {:controller=>'report',:action=>'click_gt_two',:campaign_id => report[4] }, :remote => true, :class => 'btn btn-primary' %></td>
      </tr>
      <% end %>
  </tbody>
</table>

