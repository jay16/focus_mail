<style type="text/css">     
    .digitboard{font-size:16px;width:18px;height:28px;position:relative;display:inline-block;margin:0 2px;-webkit-text-size-adjust:none;}  
    .digitboard{*display:inline;}  
    .digitboard div{position: absolute;border-style:solid;}  
    .digitboard .l,.digitboard .r,.digitboard .u,.digitboard .d{width:0;height:0;overflow:hidden;}  
      
    .digitboard .v{width:0;height:5em;border-width:0 0.5em;border-color:black;}  
    .digitboard .v .u{border-style:dotted dotted solid dotted;border-width:0 0.5em 0.5em;border-color:transparent transparent black transparent;top:-0.5em;left:-0.5em;}  
    .digitboard .v .d{border-style:solid dotted dotted dotted;border-width:0.5em 0.5em 0 0.5em;border-color:black transparent transparent transparent;left:-0.5em;bottom:-0.5em}  
      
    .digitboard .h{width:5.5em;height:0;border-width:0.5em 0;border-color:black;}  
    .digitboard .h .l{border-style:dotted solid dotted dotted;border-width:0.5em 0.5em 0.5em 0;border-color:transparent black transparent transparent;top:-0.5em;left:-0.5em;}  
    .digitboard .h .r{border-style:dotted dotted dotted solid;border-width:0.5em 0 0.5em 0.5em;border-color:transparent transparent transparent black;top:-0.5em;right:-0.5em;}  
      
    .digitboard .n1{left:0.5em;top:1.5em;}  
    .digitboard .n2{left:0.5em;top:7.5em;}  
    .digitboard .n4{left:7.5em;top:7.5em;}  
    .digitboard .n5{left:7.5em;top:1.5em;}  
      
    .digitboard .n6{top:0.4em;left:1.8em;}  
    .digitboard .n3{top:12.5em;left:1.8em;}  
    .digitboard .n7{top:6.5em;left:1.8em;}  
 
      
    .c1 .n1,.c1 .n2,.c1 .n3,.c1 .n6,.c1 .n7,  
    .c2 .n1,.c2 .n4,  
    .c3 .n1,.c3 .n2,  
    .c4 .n2,.c4 .n3,.c4 .n6,  
    .c5 .n2,.c5 .n5,  
    .c6 .n5,  
    .c7 .n1,.c7 .n2,.c7 .n3,.c7 .n7,  
    .c9 .n2,  
    .c0 .n7{border-color:#eee;}  
      
    .c1 .n3 .l,.c1 .n3 .r,.c1 .n6 .l,.c1 .n6 .r,.c1 .n7 .l,.c1 .n7 .r,  
    .c4 .n3 .l,.c4 .n3 .r,.c4 .n6 .l,.c4 .n6 .r,  
    .c7 .n3 .l,.c7 .n3 .r,.c7 .n7 .l,.c7 .n7 .r,  
    .c0 .n7 .l,.c0 .n7 .r{border-left-color:#eee;border-right-color:#eee;}  
      
    .c1 .n1 .u,.c1 .n1 .d,.c1 .n2 .u,.c1 .n2 .d,  
    .c2 .n1 .u,.c2 .n1 .d,.c2 .n4 .u,.c2 .n4 .d,  
    .c3 .n1 .u,.c3 .n1 .d,.c3 .n2 .u,.c3 .n2 .d,  
    .c4 .n2 .u,.c4 .n2 .d,  
    .c5 .n2 .u,.c5 .n2 .d,.c5 .n5 .u,.c5 .n5 .d,  
    .c6 .n5 .u,.c6 .n5 .d,  
    .c7 .n1 .u,.c7 .n1 .d,.c7 .n2 .u,.c7 .n2 .d,  
    .c9 .n2 .u,.c9 .n2 .d{border-top-color:#eee;border-bottom-color:#eee;}  

    .hide { display:none;}
    .data-label {width: 1000px; }
    </style>  
    
    
